        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.i.AndroidDemos.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.i.AndroidDemos.callbackdemo.CallBackActivity" />
        <activity android:name="com.example.i.AndroidDemos.eventdispatchdemo.EventDispatchActivity" />
        <activity android:name="com.example.i.AndroidDemos.Interreactcomponent.ActivityComponentIntereact" />
        <activity android:name="com.example.i.AndroidDemos.Interreactcomponent.FragmentToActivity.UserInfoActivity" />

        ----------------------------------笔记------------------------------------
        <activity android:name="com.example.i.AndroidDemos.noteandtools.ActivityNoteAndTools" />

        ---------------------------------网络----------------------------------
        <activity android:name="com.example.i.AndroidDemos.network.ActivityNet" />

        <service android:name="com.example.i.AndroidDemos.network.socketdemo.ServerService" />
        ---------------------------------工具----------------------------------
        <activity android:name=".noteandtools.ActivityNoteAndTools" />
    </application>

</manifest>